<?xml version="1.0" encoding="utf-8"?>
<root>
  <!-- Control Display Names -->
  <data name="TooltipControl_Display_Key" xml:space="preserve">
    <value>Input with Tooltip</value>
    <comment>Display name for the tooltip control</comment>
  </data>
  <data name="TooltipControl_Desc_Key" xml:space="preserve">
    <value>An input field with an interactive tooltip icon that can display rich content and optional redirect functionality</value>
    <comment>Description for the tooltip control</comment>
  </data>

  <!-- Main Field -->
  <data name="DummyField_Display_Key" xml:space="preserve">
    <value>Field Value</value>
    <comment>The main field that stores the input value</comment>
  </data>
  <data name="DummyField_Desc_Key" xml:space="preserve">
    <value>The field that this input control is bound to</value>
    <comment>Description for the main bound field</comment>
  </data>

  <!-- Input Field Configuration -->
  <data name="IsMultiline_Display_Key" xml:space="preserve">
    <value>Multiline Input</value>
    <comment>Whether to use a textarea instead of single line input</comment>
  </data>
  <data name="IsMultiline_Desc_Key" xml:space="preserve">
    <value>Enable multiline text input (textarea)</value>
    <comment>Description for multiline option</comment>
  </data>
  <data name="IsMultiline_True_Key" xml:space="preserve">
    <value>Yes</value>
    <comment>Multiline enabled</comment>
  </data>
  <data name="IsMultiline_False_Key" xml:space="preserve">
    <value>No</value>
    <comment>Multiline disabled</comment>
  </data>

  <data name="Placeholder_Display_Key" xml:space="preserve">
    <value>Placeholder Text</value>
    <comment>Placeholder text for the input field</comment>
  </data>
  <data name="Placeholder_Desc_Key" xml:space="preserve">
    <value>Text to display when the input is empty</value>
    <comment>Description for placeholder</comment>
  </data>

  <data name="MaxLength_Display_Key" xml:space="preserve">
    <value>Maximum Length</value>
    <comment>Maximum number of characters allowed</comment>
  </data>
  <data name="MaxLength_Desc_Key" xml:space="preserve">
    <value>Maximum number of characters allowed in the input (0 = no limit)</value>
    <comment>Description for max length</comment>
  </data>

  <data name="Rows_Display_Key" xml:space="preserve">
    <value>Textarea Rows</value>
    <comment>Number of rows for textarea</comment>
  </data>
  <data name="Rows_Desc_Key" xml:space="preserve">
    <value>Number of visible text lines for multiline input</value>
    <comment>Description for rows</comment>
  </data>

  <data name="IsReadonly_Display_Key" xml:space="preserve">
    <value>Read Only</value>
    <comment>Whether the input is read-only</comment>
  </data>
  <data name="IsReadonly_Desc_Key" xml:space="preserve">
    <value>Make the input field read-only</value>
    <comment>Description for read-only option</comment>
  </data>
  <data name="IsReadonly_True_Key" xml:space="preserve">
    <value>Yes</value>
    <comment>Read-only enabled</comment>
  </data>
  <data name="IsReadonly_False_Key" xml:space="preserve">
    <value>No</value>
    <comment>Read-only disabled</comment>
  </data>

  <!-- Input Styling -->
  <data name="InputWidth_Display_Key" xml:space="preserve">
    <value>Input Width</value>
    <comment>Width of the input field</comment>
  </data>
  <data name="InputWidth_Desc_Key" xml:space="preserve">
    <value>Width of the input field (e.g., 100%, 300px, auto)</value>
    <comment>Description for input width</comment>
  </data>

  <data name="InputHeight_Display_Key" xml:space="preserve">
    <value>Input Height</value>
    <comment>Height of the input field</comment>
  </data>
  <data name="InputHeight_Desc_Key" xml:space="preserve">
    <value>Height of the input field (e.g., auto, 40px, 2em)</value>
    <comment>Description for input height</comment>
  </data>

  <!-- Tooltip Content -->
  <data name="StaticTooltipContent_Display_Key" xml:space="preserve">
    <value>Static Tooltip Content</value>
    <comment>Static tooltip content</comment>
  </data>
  <data name="StaticTooltipContent_Desc_Key" xml:space="preserve">
    <value>Static tooltip content (up to ~4000 characters)</value>
    <comment>Description for static tooltip content</comment>
  </data>

  <data name="BoundTooltipContent_Display_Key" xml:space="preserve">
    <value>Bound Tooltip Content</value>
    <comment>Bound tooltip content field</comment>
  </data>
  <data name="BoundTooltipContent_Desc_Key" xml:space="preserve">
    <value>Bind to a field for unlimited tooltip content</value>
    <comment>Description for bound tooltip content</comment>
  </data>

  <!-- Navigation -->
  <data name="RedirectUrl_Display_Key" xml:space="preserve">
    <value>Redirect URL</value>
    <comment>URL to navigate to when icon is clicked</comment>
  </data>
  <data name="RedirectUrl_Desc_Key" xml:space="preserve">
    <value>URL to navigate to when the tooltip icon is clicked</value>
    <comment>Description for redirect URL</comment>
  </data>

  <data name="OpenInNewTab_Display_Key" xml:space="preserve">
    <value>Open in New Tab</value>
    <comment>Whether to open links in new tab</comment>
  </data>
  <data name="OpenInNewTab_Desc_Key" xml:space="preserve">
    <value>Open the redirect URL in a new browser tab</value>
    <comment>Description for new tab option</comment>
  </data>
  <data name="OpenInNewTab_True_Key" xml:space="preserve">
    <value>Yes</value>
    <comment>Open in new tab</comment>
  </data>
  <data name="OpenInNewTab_False_Key" xml:space="preserve">
    <value>No</value>
    <comment>Open in same tab</comment>
  </data>

  <!-- Icon Properties -->
  <data name="IconType_Display_Key" xml:space="preserve">
    <value>Icon Type</value>
    <comment>Type of icon to display</comment>
  </data>
  <data name="IconType_Desc_Key" xml:space="preserve">
    <value>The type of icon to display next to the input</value>
    <comment>Description for icon type</comment>
  </data>
  <data name="IconType_Info_Key" xml:space="preserve">
    <value>Info (ℹ)</value>
    <comment>Information icon</comment>
  </data>
  <data name="IconType_Question_Key" xml:space="preserve">
    <value>Question (?)</value>
    <comment>Question mark icon</comment>
  </data>
  <data name="IconType_Warning_Key" xml:space="preserve">
    <value>Warning (⚠)</value>
    <comment>Warning icon</comment>
  </data>
  <data name="IconType_Error_Key" xml:space="preserve">
    <value>Error (!)</value>
    <comment>Error icon</comment>
  </data>

  <data name="IconColor_Display_Key" xml:space="preserve">
    <value>Icon Color</value>
    <comment>Color of the tooltip icon</comment>
  </data>
  <data name="IconColor_Desc_Key" xml:space="preserve">
    <value>Color of the tooltip icon (e.g., #666666, blue, rgb(100,100,100))</value>
    <comment>Description for icon color</comment>
  </data>

  <data name="IconSize_Display_Key" xml:space="preserve">
    <value>Icon Size</value>
    <comment>Size of the tooltip icon</comment>
  </data>
  <data name="IconSize_Desc_Key" xml:space="preserve">
    <value>Size of the tooltip icon in pixels</value>
    <comment>Description for icon size</comment>
  </data>

  <!-- Tooltip Styling -->
  <data name="TooltipBackgroundColor_Display_Key" xml:space="preserve">
    <value>Tooltip Background Color</value>
    <comment>Background color of the tooltip</comment>
  </data>
  <data name="TooltipBackgroundColor_Desc_Key" xml:space="preserve">
    <value>Background color of the tooltip (e.g., #333333, darkblue)</value>
    <comment>Description for tooltip background color</comment>
  </data>

  <data name="TooltipTextColor_Display_Key" xml:space="preserve">
    <value>Tooltip Text Color</value>
    <comment>Text color of the tooltip</comment>
  </data>
  <data name="TooltipTextColor_Desc_Key" xml:space="preserve">
    <value>Text color of the tooltip (e.g., #ffffff, white)</value>
    <comment>Description for tooltip text color</comment>
  </data>

  <data name="TooltipMaxWidth_Display_Key" xml:space="preserve">
    <value>Tooltip Max Width</value>
    <comment>Maximum width of the tooltip</comment>
  </data>
  <data name="TooltipMaxWidth_Desc_Key" xml:space="preserve">
    <value>Maximum width of the tooltip in pixels (0 = default)</value>
    <comment>Description for tooltip max width</comment>
  </data>

  <data name="TooltipMaxHeight_Display_Key" xml:space="preserve">
    <value>Tooltip Max Height</value>
    <comment>Maximum height of the tooltip</comment>
  </data>
  <data name="TooltipMaxHeight_Desc_Key" xml:space="preserve">
    <value>Maximum height of the tooltip in pixels (0 = no limit)</value>
    <comment>Description for tooltip max height</comment>
  </data>

  <!-- Position -->
  <data name="Position_Display_Key" xml:space="preserve">
    <value>Tooltip Position</value>
    <comment>Position of the tooltip relative to the icon</comment>
  </data>
  <data name="Position_Desc_Key" xml:space="preserve">
    <value>Position of the tooltip relative to the icon</value>
    <comment>Description for tooltip position</comment>
  </data>
  <data name="Position_Top_Key" xml:space="preserve">
    <value>Top</value>
    <comment>Position tooltip above the icon</comment>
  </data>
  <data name="Position_Bottom_Key" xml:space="preserve">
    <value>Bottom</value>
    <comment>Position tooltip below the icon</comment>
  </data>
  <data name="Position_Left_Key" xml:space="preserve">
    <value>Left</value>
    <comment>Position tooltip to the left of the icon</comment>
  </data>
  <data name="Position_Right_Key" xml:space="preserve">
    <value>Right</value>
    <comment>Position tooltip to the right of the icon</comment>
  </data>
  <data name="Position_Auto_Key" xml:space="preserve">
    <value>Auto</value>
    <comment>Automatically position tooltip based on available space</comment>
  </data>

  <!-- Content Formatting -->
  <data name="PreserveLineBreaks_Display_Key" xml:space="preserve">
    <value>Preserve Line Breaks</value>
    <comment>Whether to preserve line breaks in tooltip content</comment>
  </data>
  <data name="PreserveLineBreaks_Desc_Key" xml:space="preserve">
    <value>Convert line breaks to &lt;br&gt; tags in tooltip content</value>
    <comment>Description for preserve line breaks</comment>
  </data>
  <data name="PreserveLineBreaks_True_Key" xml:space="preserve">
    <value>Yes</value>
    <comment>Preserve line breaks</comment>
  </data>
  <data name="PreserveLineBreaks_False_Key" xml:space="preserve">
    <value>No</value>
    <comment>Don't preserve line breaks</comment>
  </data>

  <data name="AllowHtml_Display_Key" xml:space="preserve">
    <value>Allow HTML</value>
    <comment>Whether to allow HTML in tooltip content</comment>
  </data>
  <data name="AllowHtml_Desc_Key" xml:space="preserve">
    <value>Allow HTML formatting in tooltip content (sanitized for security)</value>
    <comment>Description for allow HTML</comment>
  </data>
  <data name="AllowHtml_True_Key" xml:space="preserve">
    <value>Yes</value>
    <comment>Allow HTML</comment>
  </data>
  <data name="AllowHtml_False_Key" xml:space="preserve">
    <value>No</value>
    <comment>Don't allow HTML</comment>
  </data>
</root>